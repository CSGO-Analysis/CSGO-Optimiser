<Window x:Class="CSGO_Optimiser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="620" Width="900" MinHeight="620" MinWidth="900">
    <Grid>
        <TabControl Margin="5">
            <TabItem>
                <TabItem.Header>
                    <Label Content="Optimisation" />
                </TabItem.Header>
                <UserControl x:Name="optimisation"/>
            </TabItem>
            <TabItem>
                <TabItem.Header>
                    <Label Content="Backup" />
                </TabItem.Header>
                <UserControl x:Name="backup"/>
            </TabItem>
            <TabItem>
                <TabItem.Header>
                    <Label Content="About" />
                </TabItem.Header>
                <UserControl x:Name="About"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
